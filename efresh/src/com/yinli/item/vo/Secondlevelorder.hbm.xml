		<property name="canceltime" type="java.util.Date">
			<column name="canceltime" />
		</property>
		<property name="paytime" type="java.util.Date">
			<column name="paytime" />
		</property>
		<property name="deliverytime" type="java.util.Date">
			<column name="deliverytime" />

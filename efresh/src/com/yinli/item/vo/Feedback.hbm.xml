<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- 
    20160123
-->

<hibernate-mapping>
	<class name="com.yinli.item.vo.Feedback" table="feedback" >
		<id name="idNumber" type="java.lang.Integer">
			<column name="idNumber" />
			<generator class="native" />
		</id>
		<property name="idFeedback" type="java.lang.Integer">
			<column name="idFeedback" />
		</property>
		<property name="feedbacktime" type="java.util.Date">
			<column name="feedbacktime" />
		</property>
		<property name="feedcontent" type="java.lang.String">
			<column name="feedcontent" />
		</property>
		<property name="replycontent" type="java.lang.String">
			<column name="replycontent" />
		</property>
		<property name="idReply" type="java.lang.Integer">
			<column name="idReply" />
		</property>
		<property name="replytime" type="java.util.Date">
			<column name="replytime" />
		</property>
		<property name="ts" type="java.lang.String">
			<column name="ts" />
		</property>
		<property name="dr" type="java.lang.Integer">
			<column name="dr" />
		</property>
		<property name="back1" type="java.lang.String">
			<column name="back1" />
		</property>
	</class>
</hibernate-mapping>

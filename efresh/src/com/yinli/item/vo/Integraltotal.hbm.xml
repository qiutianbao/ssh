<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- 
    20160128
-->

<hibernate-mapping>
	<class name="com.yinli.item.vo.Integraltotal" table="integraltotal" schema="efresh">
		<id name="idNumber" type="java.lang.Integer">
			<column name="idNumber" />
			<generator class="native" />
		</id>
		<property name="idUser" type="java.lang.Integer">
			<column name="idUser" />
		</property>
		<property name="integralSum" type="java.lang.Integer">
			<column name="integralSum" />
		</property>
		<property name="usableIntegral" type="java.lang.Integer">
			<column name="usableIntegral" />
		</property>
		<property name="ts" type="java.lang.String">
			<column name="ts" />
		</property>
		<property name="dr" type="java.lang.Integer">
			<column name="dr" />
		</property>
		<property name="back1" type="java.lang.String">
			<column name="back1" />
		</property>
		<property name="back2" type="java.lang.String">
			<column name="back2" />
		</property>
		<property name="back3" type="java.lang.String">
			<column name="back3" />
		</property>
		<property name="back4" type="java.lang.String">
			<column name="back4" />
		</property>
		<property name="back5" type="java.lang.String">
			<column name="back5" />
		</property>
		<property name="validtime" type="java.util.Date">
			<column name="validtime" />
		</property>
	</class>
</hibernate-mapping>

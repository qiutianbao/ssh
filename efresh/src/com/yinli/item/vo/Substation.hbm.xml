<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!-- 
    20160125
-->

<hibernate-mapping>
	<class name="com.yinli.item.vo.Substation" table="substation" >
		<id name="idNumber" type="java.lang.Integer">
			<column name="idNumber" />
			<generator class="native" />
		</id>
		<property name="ipaddress" type="java.lang.String">
			<column name="ipaddress" />
		</property>
		<property name="dbname" type="java.lang.String">
			<column name="dbname" />
		</property>
		<property name="portNo" type="java.lang.String">
			<column name="portNo" />
		</property>
		<property name="username" type="java.lang.String">
			<column name="username" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" />
		</property>
		<property name="ts" type="java.lang.String">
			<column name="ts" />
		</property>
		<property name="dr" type="java.lang.Integer">
			<column name="dr" />
		</property>
		<property name="back1" type="java.lang.String">
			<column name="back1" />
		</property>
		<property name="back2" type="java.lang.String">
			<column name="back2" />
		</property>
		<property name="back3" type="java.lang.Double">
			<column name="back3" />
		</property>
	</class>
</hibernate-mapping>

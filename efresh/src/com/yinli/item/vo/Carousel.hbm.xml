<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    20160125
-->
<hibernate-mapping>
	<class name="com.yinli.item.vo.Carousel" table="carousel" >
		<id name="idNumber" type="java.lang.Integer">
			<column name="idNumber" />
			<generator class="native" />
		</id>
		<property name="serialnumber" type="java.lang.Integer">
			<column name="serialnumber" />
		</property>
		<property name="imagename" type="java.lang.String">
			<column name="imagename" />
		</property>
		<property name="ts" type="java.lang.String">
			<column name="ts" />
		</property>
		<property name="dr" type="java.lang.Integer">
			<column name="dr" />
		</property>
		<property name="back1" type="java.lang.String">
			<column name="back1" />
		</property>
		<property name="back2" type="java.lang.String">
			<column name="back2" />
		</property>
		<property name="back3" type="java.lang.String">
			<column name="back3" />
		</property>
	</class>
</hibernate-mapping>

<hibernate-mapping>
	<class name="com.yinli.item.vo.Bindbankcard" table="bindbankcard"   >
		<id name="idNumber" type="java.lang.Integer">
			<column name="idNumber" />
			<generator class="native" />
		</id>
		<property name="idUser" type="java.lang.Integer">
			<column name="idUser" />
		</property>
		<property name="bankCardNo" type="java.lang.String">
			<column name="bankCardNo" />
		</property>
		<property name="bankaccount" type="java.lang.String">
			<column name="bankaccount" />
		</property>
		<property name="accountname" type="java.lang.String">
			<column name="accountname" />
		</property>
		<property name="idcard" type="java.lang.String">
			<column name="idcard" />
		</property>
		<property name="iphoneNo" type="java.lang.String">
			<column name="iphoneNo" />
		</property>
		<property name="ts" type="java.lang.String">
			<column name="ts" />
		</property>
		<property name="dr" type="java.lang.Integer">
			<column name="dr" />
		</property>
		<property name="back1" type="java.lang.String">
			<column name="back1" />
		</property>
		<property name="back2" type="java.lang.String">
			<column name="back2" />
		</property>
		<property name="back3" type="java.lang.String">
			<column name="back3" />
		</property>
		<property name="back4" type="java.lang.String">
			<column name="back4" />
		</property>
		<property name="back5" type="java.lang.Double">
			<column name="back5" />
		</property>
	</class>
</hibernate-mapping>
